from days import day01, day02, day03
from advent_utils import read_input, run, test_solution


fn test_all_days() raises:
    test_solution[
        day01.Solution,
        ("../../inputs/tests/day1_1.txt", 142),
        ("../../inputs/tests/day1_2.txt", 281),
    ]()
    test_solution[
        day02.Solution,
        ("../../inputs/tests/day2_1.txt", (8, 2286)),
    ]()
    test_solution[
        day03.Solution,
        ("../../inputs/tests/day3_1.txt", (4361, 467835)),
    ]()
